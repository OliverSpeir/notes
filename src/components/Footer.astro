    <a
      href="http://example.com"
      target="_blank"
      rel="noopener noreferrer"
      class="m-1 md:m-4"
      aria-label="Email"
    >
      <svg
        xmlns="mailto:example@example.com"
        fill="none"
        viewBox="0 0 24 24"
        stroke-width="1.5"
        stroke="currentColor"
        class="w-7 h-7 stroke-black dark:stroke-white"
      >
