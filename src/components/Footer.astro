---
import type { CollectionEntry } from "astro:content";
type Props = {note: CollectionEntry<"notes">};
const { note } = Astro.props;
---
{note.data.footer && 
<div class="w-1/3 mx-auto border-t border-zinc-500 mt-10 hidden xl:flex"></div>
<footer class="flex justify-center items-center z-20 relative pb-4 lg:pb-2">
